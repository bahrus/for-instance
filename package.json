    "xtal-element": "0.0.442",
    "xtal-fetch": "0.0.108",
    "pass-prop": "0.0.35",
    "trans-render": "0.0.412",
    "xtal-editor": "0.0.59",
    "pass-down": "0.0.116",
    "if-diff": "0.0.117",
    "xtal-shell": "0.0.25",
    "ib-id": "0.0.79",
    "aggregator-fn": "0.0.37",
    "ref-to": "0.0.8",
    "wc-info": "0.0.125"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.57",
