{
  "name": "for-instance",
  "version": "0.0.45",
  "keywords": [
    "web-components",
    "custom-elements",
    "micro-frontends",
    "integration-testing"
  ],
  "description": "Push for the Swaggerization of Web Components",
  "main": "for-instance.js",
  "module": "for-instance.js",
  "types": "types.d.ts",
  "scripts": {
    "test": "node test/test-all.js",
    "package-check": "package-check",
    "serve": "node node_modules/xtal-test/serve.js",
    "doc": "custom-elements-manifest analyze"
